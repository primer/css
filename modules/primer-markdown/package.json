{
  "version": "3.7.0",
  "name": "primer-markdown",
  "description": "GitHub stylesheets for rendering markdown.",
  "homepage": "https://github.com/primer/primer-markdown",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "style": "index.scss",
  "main": "build/index.js",
  "primer": {
    "category": "product",
    "module_type": "components"
  },
  "files": [
    "index.scss",
    "lib",
    "build"
  ],
  "repository": "https://github.com/primer/primer-css/tree/master/modules/primer-markdown",
  "bugs": {
    "url": "https://github.com/primer/primer-css/issues"
  },
  "scripts": {
    "build": "primer-module-build index.scss",
    "prepare": "npm run build",
    "lint": "stylelint **/*.scss -s scss",
    "test": "npm-run-all -s build lint"
  },
  "dependencies": {
    "primer-support": "4.4.0"
  },
  "keywords": [
    "github",
    "markdown",
    "css",
    "scss",
    "primer",
    "style",
    "stylesheets",
    "md"
  ]
}
