{
  "version": 2,
  "name": "primer-css",
  "alias": "primer-css.vercel.app",
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    }
  ],
  "routes": [
    {
      "src": "/",
      "status": 301,
      "headers": {"Location": "/css"}
    },
    {
      "src": "/css/objects/layout/?",
      "status": 301,
      "headers": {"Location": "/css/objects/grid"}
    },
    {
      "src": "/css(/.*)?",
      "dest": "/docs$1"
    }
  ],
  "github": {
    "enabled": false
  }
}
