{
  "name": "@primer/css",
  "version": "16.0.2",
  "description": "Primer is the CSS framework that powers GitHub's front-end design. primer includes 23 packages that are grouped into 3 core meta-packages for easy install. Each package and meta-package is independently versioned and distributed via npm, so it's easy to include all or part of Primer within your own project.",
  "homepage": "https://primer.style/css",
  "author": "GitHub, Inc.",
  "license": "MIT",
  "style": "dist/primer.css",
  "sass": "index.scss",
  "main": "dist/primer.js",
  "repository": "https://github.com/primer/css",
  "bugs": {
    "url": "https://github.com/primer/css/issues"
  },
  "keywords": [
    "primer",
    "css",
    "github",
    "design-system"
  ],
  "scripts": {
    "fresh": "rm -rf node_modules; npm install",
    "dist": "script/dist.js",
    "lint": "npm-run-all -s stylelint eslint eslint-docs",
    "stylelint": "stylelint --quiet src",
    "eslint": "eslint script deprecations.js lib .storybook",
    "eslint-docs": "eslint docs/{content,src,*.js}",
    "prepublishOnly": "script/prepublish",
    "publish-storybook": "script/publish-storybook",
    "start": "npm run dev",
    "dev": "cd docs && npm install && npm run develop",
    "start-storybook": "start-storybook -p 8001",
    "build-storybook": "build-storybook -o .storybuild",
    "test": "npm-run-all -s test-urls",
    "test-urls": "script/test-doc-urls.js",
    "release": "changeset publish"
  },
  "dependencies": {
    "@primer/octicons": "^9.1.1",
    "@primer/primitives": "^4.0.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.3.0",
    "@changesets/cli": "^2.14.1",
    "@storybook/addon-viewport": "6.1.21",
    "@storybook/react": "6.1.21",
    "autoprefixer": "10.2.5",
    "code-blocks": "^1.1.0",
    "colorette": "^1.2.2",
    "css-loader": "5.2.0",
    "cssstats": "4.0.2",
    "eslint": "7.22.0",
    "eslint-plugin-github": "4.1.2",
    "filesize": "6.1.0",
    "fs-extra": "^9.1.0",
    "gh-pages": "^3.1.0",
    "globby": "^11.0.3",
    "klaw": "3.0.0",
    "minimist": "1.2.5",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.2.8",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "3.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-node-sass": "2.1.8",
    "postcss-scss": "3.0.5",
    "postcss-value-parser": "^4.1.0",
    "prop-types": "^15.6.2",
    "semver": "7.3.5",
    "style-loader": "^2.0.0",
    "styled-components": "5.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-primer": "^11.0.1",
    "stylelint-disable": "^0.1.5",
    "stylelint-only": "^1.0.1",
    "stylelint-scss": "^3.19.0",
    "table": "6.0.7",
    "typographic-base": "^1.0.4",
    "unified": "^9.2.1",
    "unist-util-select": "^3.0.4",
    "unist-util-stringify-position": "^2.0.3"
  }
}
