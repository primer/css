{
  "name": "stylelint-config-primer",
  "version": "2.1.0",
  "description": "Sharable stylelint config used by GitHub's CSS",
  "main": "index.js",
  "scripts": {
    "ava": "ava --verbose \"tests/**/*.js\"",
    "lint": "eslint **/*.js",
    "test": "npm run lint && npm run ava"
