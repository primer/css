{
  "version": 2,
  "name": "primer-css",
  "alias": "primer-css.now.sh",
  "routes": [
    {"src": "(.+)/", "dest": "/css/$1"}
  ],
  "files": [
    "docs",
    "lib",
    "next.config.js",
    "package.json",
    "package-lock.json",
    "postcss.config.js",
    "pages",
    "script",
    "src",
    "static"
  ],
  "scale": {
    "sfo": {
      "min": 1,
      "max": 3
    }
  },
  "engines": {
    "node": "10"
  }
}
